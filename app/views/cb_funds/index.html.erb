<p id="notice"><%= notice %></p>

<h1>Cb Funds</h1>

<table>
  <thead>
    <tr>
      <th>Fund</th>
      <th>Object</th>
      <th>Name</th>
      <th>Funded at</th>
      <th>Raised amount</th>
      <th>Raised currency code</th>
      <th>Source url</th>
      <th>Source description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cb_funds.each do |cb_fund| %>
      <tr>
        <td><%= cb_fund.fund %></td>
        <td><%= cb_fund.object %></td>
        <td><%= cb_fund.name %></td>
        <td><%= cb_fund.funded_at %></td>
        <td><%= cb_fund.raised_amount %></td>
        <td><%= cb_fund.raised_currency_code %></td>
        <td><%= cb_fund.source_url %></td>
        <td><%= cb_fund.source_description %></td>
        <td><%= link_to 'Show', cb_fund %></td>
        <td><%= link_to 'Edit', edit_cb_fund_path(cb_fund) %></td>
        <td><%= link_to 'Destroy', cb_fund, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cb Fund', new_cb_fund_path %>
